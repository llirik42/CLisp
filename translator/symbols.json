{
  "api": {
    "display": "cl_display",
    "+": "cl_add",
    "-": "cl_sub",
    "*": "cl_mul",
    "/": "cl_div",
    ">": "cl_greater",
    ">=": "cl_greater_or_equal",
    "<": "cl_less",
    "<=": "cl_less_or_equal",
    "=": "cl_equal",
    "not": "cl_not"
  },
  "internal": {
    "functions": {
      "integer" : "cl_make_int",
      "true" : "cl_make_true",
      "false": "cl_make_false",
      "character" : "cl_make_char",
      "string" : "cl_make_string",
      "float" : "cl_make_double",
      "list": "cl_make_list_from_array",
      "unspecified": "cl_make_unspecified",
      "evaluable": "cl_make_evaluable",
      "lambda": "cl_make_lambda",
      "~object": "cl_destroy_obj",
      "environment": "cl_make_env",
      "environment_global": "cl_make_global_env",
      "~environment": "cl_destroy_env",
      "set_variable_value": "cl_set_variable_value",
      "update_variable_value": "cl_update_variable_value",
      "get_variable_value": "cl_get_variable_value",
      "lambda_call": "cl_lambda_call",
      "to_boolean": "cl_obj_to_boolean",
      "ref_count++": "cl_increase_refs_count"
    },
    "types": {
      "object": "CL_Object*",
      "environment": "CL_Environment*",
      "lambda_function_params": ["CL_Environment* env", "unsigned int count", "CL_Object** args"]
    }
  }
}
